
<%- include("partials/header") %>


<p><%= startingConent %> </p>
<%if(posts.length>0){ %>

   <% posts.forEach(element => { %>

    <h1><%= element.title %> </h1>

   <% if(element.content.length>100){ %>
        <p><%= element.content.slice(0,100).concat(" ...")%><a href="/posts/<%=element.title %>">Read More</a> </p>
   <% }else{  %>
        <p><%= element.content%></p>
  <%  } %>
    
   
       
  <%  });  %>

<%};%>

<%- include("partials/footer") %>
